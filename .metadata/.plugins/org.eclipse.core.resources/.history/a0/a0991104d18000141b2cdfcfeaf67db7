import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;


public class ModernRock {
	public void list5() throws FileNotFoundException {
		
	Scanner fileReader = new Scanner(new File("input.txt"));
	
	System.out.println(""
			+ "");
	
	System.out.println("Modern Rock:");
	
	do {
		String song = fileReader.nextLine();
		Scanner input = new Scanner (song);
		
		if (song.contains("Rock") && song.contains("1990") || song.contains("1991") || song.contains("1992") || song.contains("1993") || song.contains("1994") || song.contains("1995") || song.contains("1996") || song.contains("1997") || song.contains("1998") || song.contains("1999") || song.contains("2000") || song.contains("2001") || song.contains("2002") || song.contains("2003") || song.contains("2004") || song.contains("2005") || song.contains("2006") || song.contains("2007") || song.contains("2008") || song.contains("2009") || song.contains("2010") || song.contains("2011") || song.contains("2012") || song.contains("2013") || song.contains("2014")) {
			input.next();
		System.out.println(song);
		}
		
	} while (fileReader.hasNextLine());
	
	fileReader.close();
	}
}
