import java.util.*;
import java.io.*;

/**
 * 
 */

/**
 * @author Jordan
 *
 */
public class Analysis {
	Scanner fileReader = new Scanner (new File("src/input.txt"));
	int numLines = 0;

	int i = 0;
	int counter = 0;
	
	while (fileReader.hasNextLine()) {
		fileReader.nextLine();
		numLines++;
		fileReader.close();
	}
	int[][] cars = new int [numLines][2];
		while (fileReader.hasNextLine()) {
			fileReader.nextLine();
			int year = fileReader.nextInt();
			int mileage = fileReader.nextInt();
			cars[i][0] = year;
			cars[i][1] = mileage;
		}
		fileReader.close();
		}
	}
}