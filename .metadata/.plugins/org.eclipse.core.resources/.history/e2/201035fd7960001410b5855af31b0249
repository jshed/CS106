import java.util.*;
import java.io.*;


public class Analysis {
	public void organize(File file) throws FileNotFoundException{
	int numLines = 0;
	Car[] car = new Car[numLines];
	Scanner fileReader = new Scanner (new File("src/input.txt"));

	int i = 0;
	int counter = 0;
	
	while (fileReader.hasNextLine()) {
		fileReader.nextLine();
		numLines++;
		fileReader.close();
	}
	int[][] cars = new int [numLines][2];
		while (fileReader.hasNextLine()) {
			fileReader.nextLine();
			int year = fileReader.nextInt();
			int mileage = fileReader.nextInt();
			cars[i][0] = year;
			cars[i][1] = mileage;
			i++;
		fileReader.close();
		}
	for(int k = 0; k < numLines; k++) {
		if(cars[k][0] >= 2005) {
			if(cars[k][1] >= 75000){
				counter++;
			}
		}
	}
}