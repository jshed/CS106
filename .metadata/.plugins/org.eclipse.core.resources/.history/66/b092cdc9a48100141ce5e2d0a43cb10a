import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;


public class ModernRock {
	public void list5() throws FileNotFoundException {
		
	Scanner fileReader = new Scanner(new File("input.txt"));
	
	System.out.println(""
			+ "");
	
	System.out.println("Modern Rock:");
	
	do {
		String song = fileReader.nextLine();
		Scanner input = new Scanner (song);
		
		if (song.contains("Rock") && !song.contains("Alternative") && song.contains("1991") || song.contains("1995") || song.contains("1999") || song.contains("2001") || song.contains("2004") || song.contains("2005") || song.contains("2007") || song.contains("2008")) {
			input.next();
		System.out.println(song);
		}
		
	} while (fileReader.hasNextLine());
	
	fileReader.close();
	}
}
