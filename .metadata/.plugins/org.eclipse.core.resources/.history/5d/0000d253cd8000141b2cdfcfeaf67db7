
public class Playlist {
	public String song;
	public String band;
	public String album;
	public String genre;
	public int year;
	
	public Playlist(String song, String band, String album, String genre, int year) {
		this.song = song;
		this.band = band;
		this.album = album;
		this.genre = genre;
		this.year = year;
	}

}
