import java.util.Scanner;
import java.io.*;

/**
 * 
 */

/**
 * @author Jordan
 *
 */
public class ProjectFive {

	private static final boolean[] String = null;

	/**
	 * @param args
	 */
	public static void main(String[] args) throws FileNotFoundException {
		Scanner fileReader = new Scanner (new File("src/input.txt"));
		int numLines = 0;
		while (fileReader.hasNextLine()) {
			fileReader.nextLine();
			numLines++;
		}
		fileReader.close();
		
		System.out.println(numLines);
		
		int [] year = new int [numLines];
		String [] make = new String [numLines];
		int [] mileage = new int [numLines];
		
		int i = 0;
		fileReader = new Scanner(new File("src/input.txt"));
		while (fileReader.hasNext()) {
			year [i] = fileReader.next();
			make [i] = fileReader.next();
			mileage [i] = fileReader.next();
			i++;
		}
		fileReader.close();
		
	}
}