import java.util.*;
import java.io.*;


public class Analysis {
	public void organize(File file) throws FileNotFoundException{
	Scanner fileReader = new Scanner (new File("src/input.txt"));
	int numLines = 0;
	while (fileReader.hasNextLine()) {
		fileReader.nextLine();
		numLines++;
	}
	fileReader.close();
	Car[] cars = new Car[numLines];
	
	int counter = 0;

	int i = 0;
	int year;
	String make;
	int mileage;
	
	while (fileReader.hasNextLine()) {
		fileReader.nextLine();
		year = fileReader.nextInt();
		make = fileReader.toString();
		mileage = fileReader.nextInt();
		
		cars[i] = new Car(year, make, mileage);
		i++;
	}
	fileReader.close();
	
		for(int j = 0; j < numLines ; j++) {
			if(cars[j].year >= 2005 ){
				if(cars[j].mileage >= 75000) {
					counter++;
		System.out.println("Cars newer than 2005 with over 75,000 miles: " + counter);
			
	
				}
			}
		}
	}
}